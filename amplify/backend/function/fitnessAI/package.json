{
  "name": "fitness-ai-function",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "esbuild src/index.ts --bundle --minify --sourcemap --platform=node --outfile=dist/index.js",
    "watch": "esbuild src/index.ts --bundle --minify --sourcemap --platform=node --outfile=dist/index.js --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "^2.0.0",
    "@aws-lambda-powertools/tracer": "^2.0.0",
    "openai": "^4.20.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.0",
    "@types/node": "^20.10.0",
    "@vitest/coverage-v8": "^2.1.0",
    "esbuild": "^0.19.0",
    "typescript": "^5.3.0",
    "vitest": "^2.1.0"
  }
}
